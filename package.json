{
  "name": "shengshu-react-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "prebuild": "node imagemin.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --cache --check **/**/*.{tsx,ts,js}",
    "prettier:fix": "prettier --cache --write **/**/*.{tsx,ts,js}",
    "stylelint": "npx stylelint **/*{.css,.scss} --cache",
    "stylelint:fix": "npx stylelint **/*{.css,.scss} --fix --cache",
    "eslint": "npx eslint src --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npx eslint src --fix --cache --ext  .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lintfix": "yarn prettier:fix\nyarn stylelint:fix\nyarn eslint:fix",
    "ytt": "npx ytt"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache"
    ],
    "**/*.{js,jsx,ts,tsx,cjs,less,scss,css,md}": [
      "prettier --write --cache"
    ],
    "**/*.{less,scss,css}": [
      "stylelint --fix --cache"
    ]
  },
  "keywords": [
    "vite",
    "scss",
    "css modules",
    "react18.x",
    "hooks",
    "redux"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@lottiefiles/react-lottie-player": "^3.4.7",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/js-cookie": "^2.2.1",
    "@types/react-css-modules": "^4.6.4",
    "ahooks": "^3.6.2",
    "antd": "^5.3.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "js-cookie": "^2.2.1",
    "react": "^18.0.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "sass": "^1.51.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.3.0",
    "chalk": "^5.2.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "globby": "^13.1.3",
    "husky": "^8.0.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^13.2.0",
    "ora": "^6.3.0",
    "prettier": "^2.7.0",
    "redux-logger": "^3.0.6",
    "slash": "^5.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "yapi-to-typescript": "^3.34.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 11"
    ]
  }
}
